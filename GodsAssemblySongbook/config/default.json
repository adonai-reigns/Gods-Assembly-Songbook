{
    "version": "1.2.0",
    "fileUploads": {
        "directory": "./.database/uploads",
        "wallpapers": {
            "storageDirectory": "wallpapers",
            "validTypes": [
                "jpg",
                "jpeg",
                "png",
                "gif",
                "webp",
                "mkv",
                "mp4",
                "webm",
                "ogg",
                "ogx"
            ],
            "maxFileSizeMB": 80,
            "maxParallelUploadCount": 50
        }
    },
    "imports": {
        "tempDirectory": "./temp",
        "maxFileSizeMB": 80,
        "maxParallelUploadCount": 50,
        "songs": {
            "filename": "Gods-Assembly-Songbook_ExportedSongs_{date}.{ext}"
        },
        "playlists": {
            "filename": "Gods-Assembly-Songbook_ExportedPlaylists_{date}.{ext}"
        }
    },
    "exports": {
        "songs": {
            "filename": "Gods-Assembly-Songbook_ExportedSongs_{date}.{ext}"
        },
        "playlists": {
            "filename": "Gods-Assembly-Songbook_ExportedPlaylists_{date}.{ext}"
        }
    }
}
